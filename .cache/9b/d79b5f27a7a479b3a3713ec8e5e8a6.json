original":{"line":72,"column":7}},{"generated":{"line":68,"column":0},"source":"inputAdd.js","original":{"line":73,"column":5}},{"generated":{"line":68,"column":5},"source":"inputAdd.js","original":{"line":23,"column":4}},{"generated":{"line":69,"column":0},"source":"inputAdd.js","original":{"line":74,"column":3}},{"generated":{"line":69,"column":3},"source":"inputAdd.js","original":{"line":6,"column":2}},{"generated":{"line":70,"column":0},"source":"inputAdd.js","original":{"line":75,"column":1}},{"generated":{"line":70,"column":1},"source":"inputAdd.js","original":{"line":3,"column":7}}],"sources":{"inputAdd.js":"/* eslint-disable */\r\n\r\nexport const inputAddFiles = () => {\r\n  const addButtons = document.querySelectorAll('.btn--add');\r\n\r\n  addButtons.forEach(button => {\r\n    const hiddenInput = button.parentElement.querySelector(\r\n      '.file__upload--input'\r\n    );\r\n    const label = button.parentElement.parentElement.querySelector(\r\n      '.file__upload--label'\r\n    );\r\n    const defaultLabelText = 'Nothing selected.';\r\n\r\n    //set default label text\r\n    label.textContent = defaultLabelText;\r\n\r\n    //trigger click event on the hidden input\r\n    button.addEventListener('click', e => {\r\n      hiddenInput.click();\r\n    });\r\n\r\n    hiddenInput.addEventListener('change', () => {\r\n      const fileNameList = Array.from(hiddenInput.files).map(file => {\r\n        return file.name;\r\n      });\r\n      const addTag = () => {\r\n        const parent = button.parentElement.parentElement.querySelector(\r\n          '.tags'\r\n        );\r\n        const appendTag = limit => {\r\n          let tagList = Array.from(\r\n            parent.querySelectorAll('.tag--description')\r\n          ).map(el => {\r\n            return el.textContent;\r\n          });\r\n          console.log(tagList);\r\n          if (tagList.length < limit) {\r\n            for (let i = 0; i < fileNameList.length; i++) {\r\n              if (!tagList.includes(fileNameList[i])) {\r\n                const tag = document.createElement('div');\r\n                tag.className = 'tag';\r\n                tag.title = fileNameList[i];\r\n                tag.innerHTML = `\r\n                    <div class=\"tag--description\">${fileNameList[i]}</div>\r\n                    <svg class=\"icon--close\">\r\n                      <use xlink:href=\"/img/icons.svg#icon-x-circle\"></use></svg>`;\r\n\r\n                parent.appendChild(tag);\r\n              } else {\r\n                console.log('same files was added');\r\n              }\r\n            }\r\n          } else {\r\n            console.log(tagList, 'error: limit exceeded');\r\n          }\r\n        };\r\n        // limiting for single and triple tag\r\n        if (parent.classList.contains('single--tag')) {\r\n          appendTag(1);\r\n        } else if (parent.classList.contains('triple--tag')) {\r\n          appendTag(3);\r\n        } else {\r\n          //In case we don't want to limit(it's bad i know) we put Infinity as argument for testing\r\n          appendTag(Infinity);\r\n        }\r\n        label.textContent = '';\r\n        // fileNameList.forEach(el => {});\r\n      };\r\n      if (fileNameList.length > 0) {\r\n        addTag();\r\n      }\r\n    });\r\n  });\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"96d92b16ba89ad358e71a7414f96b110","cacheData":{"env":{}}}